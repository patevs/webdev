{
  "name": "plunkr-project",
  "version": "0.7.0",
  "description": "angularjs plunkr info project",
  "author": "Patrick Evans",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules app/lib",
    "preinstall": "rm -rf node_modules/*/.git",
    "postinstall": "pnpm run copy-libs",
    "update-deps": "pnpm update",
    "postupdate-deps": "pnpm run copy-libs",
    "copy-libs": "cpx \"node_modules/{angular,angular-*,bootstrap/dist,bootstrap-*/dist,jquery/dist}/**/*\" app/lib -C",
    "prestart": "pnpm install",
    "start": "http-server -a localhost -p 8000 -c-1 ./app",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "angular": "^1.4.9",
    "angular-ui-bootstrap": "^1.3.3",
    "bootstrap": "^3.3.7",
    "bootstrap-select": "1.13.9",
    "bootstrap-table": "^1.14.2",
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "eslint": "^5.16.0",
    "eslint-config-angular": "^0.5.0",
    "eslint-plugin-angular": "^4.0.0",
    "http-server": "^0.11.1"
  }
}
