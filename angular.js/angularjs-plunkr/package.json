{
  "name": "angularjs-plunkr",
  "version": "1.0.0",
  "description": "angularjs plunkr",
  "author": "Patrick Evans",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "preinstall": "rm -rf node_modules/*/.git",
    "postinstall": "npm run copy-libs",
    "update-deps": "npm update",
    "postupdate-deps": "npm run copy-libs",
    "copy-libs": "cpx \"node_modules/{angular,angular-*,bootstrap3/dist,jquery/dist}/**/*\" app/lib -C",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8000 -c-1 ./app",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "angular": "^1.4.14",
    "angular-ui-bootstrap": "^1.3.3",
    "bootstrap3": "^3.3.5",
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "http-server": "^0.11.1"
  }
}
